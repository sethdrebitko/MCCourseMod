{
  "schemaVersion": 1,
  "id": "mccourse",
  "version": "${version}",

  "name": "MCCourse",
  "description": "A mod to teach others fabric Minecraft Modding.",
  "authors": [
    "Seth Drebitko"
  ],
  "contact": {
    "homepage": "https://vanillaexpanded.com/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.sethdrebitko.mccourse.MCCourseMod"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.12.12",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
